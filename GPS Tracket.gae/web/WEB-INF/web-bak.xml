<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.4//EN"
"http://java.sun.com/dtd/web-app_2_4.dtd">

<web-app>

    <welcome-file-list>
        <welcome-file>google_app_engine.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>gps-coordinates</servlet-name>
        <servlet-class>net.gps.tracker.servlet.SetUserLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-location</servlet-name>
        <servlet-class>net.gps.tracker.servlet.GetUserLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-location</servlet-name>
        <servlet-class>net.gps.tracker.servlet.GetUsersLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-map</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UserMap</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-map</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UsersMap</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-kml</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UserKml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-kml</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UsersKml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-html</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UserHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-html</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UsersHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-kml-html</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UserKmlHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-kml-html</servlet-name>
        <servlet-class>net.gps.tracker.servlet.UsersKmlHtml</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>gps-coordinates</servlet-name>
        <url-pattern>/coordinates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-location</servlet-name>
        <url-pattern>/list/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-location</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-map</servlet-name>
        <url-pattern>/map/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-map</servlet-name>
        <url-pattern>/map</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-kml</servlet-name>
        <url-pattern>/kml/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-kml</servlet-name>
        <url-pattern>/kml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-html</servlet-name>
        <url-pattern>/m/u</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-html</servlet-name>
        <url-pattern>/m</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-kml-html</servlet-name>
        <url-pattern>/kml-html/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-kml-html</servlet-name>
        <url-pattern>/kml-html</url-pattern>
    </servlet-mapping>
</web-app>
