<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet-mapping>
        <servlet-name>NewUser</servlet-name>
        <url-pattern>/NewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListUsers</servlet-name>
        <url-pattern>/ListUsers</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>google_app_engine.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>gps-coordinates</servlet-name>
        <servlet-class>net.gps.tracker.web.SetUserLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-location</servlet-name>
        <servlet-class>net.gps.tracker.web.GetUserLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-location</servlet-name>
        <servlet-class>net.gps.tracker.web.GetUsersLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-map</servlet-name>
        <servlet-class>net.gps.tracker.web.UserMap</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-map</servlet-name>
        <servlet-class>net.gps.tracker.web.UsersMap</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-kml</servlet-name>
        <servlet-class>net.gps.tracker.web.UserKml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-kml</servlet-name>
        <servlet-class>net.gps.tracker.web.UsersKml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-html</servlet-name>
        <servlet-class>net.gps.tracker.web.UserHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-html</servlet-name>
        <servlet-class>net.gps.tracker.web.UsersHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-user-kml-html</servlet-name>
        <servlet-class>net.gps.tracker.web.UserKmlHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gps-users-kml-html</servlet-name>
        <servlet-class>net.gps.tracker.web.UsersKmlHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUser</servlet-name>
        <servlet-class>net.gps.tracker.web.NewUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListUsers</servlet-name>
        <servlet-class>net.gps.tracker.web.ListUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gps-coordinates</servlet-name>
        <url-pattern>/coordinates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-location</servlet-name>
        <url-pattern>/list/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-location</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-map</servlet-name>
        <url-pattern>/map/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-map</servlet-name>
        <url-pattern>/map</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-kml</servlet-name>
        <url-pattern>/kml/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-kml</servlet-name>
        <url-pattern>/kml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-html</servlet-name>
        <url-pattern>/m/u</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-html</servlet-name>
        <url-pattern>/m</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-user-kml-html</servlet-name>
        <url-pattern>/kml-html/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gps-users-kml-html</servlet-name>
        <url-pattern>/kml-html</url-pattern>
    </servlet-mapping>
</web-app>
